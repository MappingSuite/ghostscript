				sh 'cd $WORKSPACE/src/ghostscript-9.52 && ./configure && make'
				
				}
        }	

		stage('Deploy') {

            steps {
				echo "###############################################################################################"
				echo "#                                          DEPLOY                                             #"
				echo "###############################################################################################"

				
			}
		}		
	}
}
